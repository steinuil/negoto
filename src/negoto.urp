database dbname=negoto
allow meta *
allow url https://github.com/steinuil/negoto
allow url /static/*
allow env NEGOTO_LOGLEVEL
allow responseHeader Status
rewrite style Negoto/* [-]
rewrite style Post/* text_ [-]
rewrite url * [-]
timeFormat %Y/%m/%d %H:%M:%S
allow mime text/css
allow mime text/plain
library file
library post
library uuid
ffi tags

